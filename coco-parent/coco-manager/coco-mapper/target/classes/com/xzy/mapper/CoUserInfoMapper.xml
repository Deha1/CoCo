<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xzy.mapper.CoAddressMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.xzy.pojo.CoUserInfo">
        <result column="id" property="id" />
        <result column="user_id" property="userId" />
        <result column="user_name" property="userName" />
        <result column="user_pic" property="userPic" />
        <result column="sex" property="sex" />
        <result column="phone" property="phone" />
        <result column="brithday" property="brithday" />
        <result column="mood" property="mood" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
    </resultMap>

    <sql id="Base_Column_List">
    id, user_id, user_name, user_pic, sex, phone, brithday, mood, create_time, update_time
  </sql>

</mapper>
